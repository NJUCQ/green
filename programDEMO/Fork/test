#include <stdio.h>
#include <unistd.h>
#include <sys/stat.h> 
#include <stdlib.h>
#include <pthread.h>
#include <dirent.h>
#include <string.h>

int main(int argc,char* argv[])
{
	int fd = open("ps.out",O_WRONLY|O_CREAT|O_TRUNC,0644);
	if(fd<0){
	perror("open ps.out error");
}
	dup2(fd,STDOUT_FILENO);

	execlp("ps","ps","aux",NULL);
	perror("execlp error");

	//close(fd);   no work
	return 0;

}
